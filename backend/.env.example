# Security
SECRET_KEY=your_secret_key_here

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/agent_learning

# Blockchain Configuration
WEB3_PROVIDER_URI=http://127.0.0.1:8545
CHAIN_ID=1337