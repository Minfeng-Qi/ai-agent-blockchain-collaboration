# Example environment variables file
# Copy this to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=True

# Agent Configuration
AGENT_MOCK_MODE=False
OPENAI_DEFAULT_MODEL=gpt-3.5-turbo

# CORS Configuration
